    <formulas>
        <name>CalculateRPMScore</name>
        <dataType>Number</dataType>
        <expression>(CASE({!$Record.New_Stakeholder__c},&quot;Yes&quot; , 10 ,&quot;No&quot;, 20 ,0))+
(CASE({!$Record.Is_the_Dealer_meeting_sales_objectives__c},&quot;Yes&quot; , 20 ,&quot;No&quot;, 10 ,0))+
(CASE({!$Record.Dealer_is_Satisfied_with_the_partnership__c},&quot;Yes&quot; , 20 ,&quot;No&quot;, 10 ,0))+
(CASE({!$Record.Decision_Maker_attended_Performance_call__c},&quot;Yes&quot; , 20 ,&quot;No&quot;, 10 ,0))+
(CASE({!$Record.third_Party_vendor_consistent_involvemen__c},&quot;Yes&quot; , 20 ,&quot;No&quot;, 10 ,0))</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>CountDays</name>
        <dataType>Number</dataType>
        <expression>TODAY() - {!$Record.Close_Date__c}</expression>
        <scale>2</scale>
    </formulas>
