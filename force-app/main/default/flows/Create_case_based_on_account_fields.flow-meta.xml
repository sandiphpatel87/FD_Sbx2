    <formulas>
        <name>Actualgooglelaunchdateplus14days</name>
        <dataType>Date</dataType>
        <expression>{!$Record.Actual_Google_Launch_Date__c} + 14</expression>
    </formulas>
    <formulas>
        <name>Actualgooglelaunchdateplus1businessday</name>
        <dataType>Date</dataType>
        <expression>{!$Record.Actual_Google_Launch_Date__c} + 1</expression>
    </formulas>
    <formulas>
        <name>Actualgooglelaunchdateplus7days</name>
        <dataType>Date</dataType>
        <expression>{!$Record.Actual_Google_Launch_Date__c} + 7</expression>
    </formulas>
    <formulas>
        <name>Checkdayoftoday</name>
        <dataType>String</dataType>
        <expression>CASE(
  MOD( Today() - DATE( 1900, 1, 7 ), 7 ),
  0, &quot;Sunday&quot;,
  1, &quot;Monday&quot;,
  2, &quot;Tuesday&quot;,
  3, &quot;Wednesday&quot;,
  4, &quot;Thursday&quot;,
  5, &quot;Friday&quot;, 
  &quot;Saturday&quot;
)</expression>
    </formulas>
    <formulas>
        <name>FacebookNotYetLivesinceactualgooglelaunchdate</name>
        <dataType>String</dataType>
        <expression>&quot;Facebook Not Yet Live since &quot; &amp; Text({!$Record.Actual_Google_Launch_Date__c})</expression>
    </formulas>
    <formulas>
        <name>Firstdateofmonth</name>
        <dataType>Date</dataType>
        <expression>DATE(YEAR(TODAY()), MONTH(TODAY()),1)</expression>
    </formulas>
    <formulas>
        <name>NCMcheck</name>
        <dataType>Boolean</dataType>
        <expression>AND(INCLUDES({!$Record.Type__c}, &apos;NCM&apos;), NOT(INCLUDES({!$Record.Type__c}, &apos;NCM 20 Group&apos;)) )</expression>
    </formulas>
    <formulas>
        <name>onedayafteractualgooglelaunchdate</name>
        <dataType>Date</dataType>
        <expression>{!$Record.Actual_Google_Launch_Date__c} + 1</expression>
    </formulas>
