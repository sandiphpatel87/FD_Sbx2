if(Contains({!recordId.Profit_Center_New__c}, &quot;Display;&quot;), &quot;Display New;&quot;, &quot;&quot;)&amp;
if(Contains({!recordId.Profit_Center_New__c}, &quot;Dealer Name Only;&quot;), &quot;Search New;Video New;&quot;, &quot;&quot;)&amp;

if(Contains({!recordId.Profit_Center_Service__c}, &quot;Google;&quot;) || Contains({!recordId.Profit_Center_Service__c}, &quot;Bing;&quot;), &quot;Search Used;VLA Used;&quot;, &quot;&quot;)&amp;
if(Contains({!recordId.Profit_Center_Service__c}, &quot;Display;&quot;), &quot;Display Used;&quot;, &quot;&quot;)&amp;
if(Contains({!recordId.Profit_Center_Service__c}, &quot;YouTube;&quot;), &quot;Video Used;&quot;, &quot;&quot;)&amp;

if(Contains({!recordId.Profit_Center_Used__c}, &quot;Google;&quot;) || Contains({!recordId.Profit_Center_Used__c}, &quot;Bing;&quot;), &quot;Search Service;&quot;, &quot;&quot;)&amp;

if(Contains({!recordId.Profit_Center_New__c}, &quot;Facebook;&quot;) || Contains({!recordId.Profit_Center_Service__c}, &quot;Facebook;&quot;), &quot;Social;&quot;, &quot;&quot;)</expression>
    </formulas>
