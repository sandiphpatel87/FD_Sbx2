    <formulas>
        <name>CalculatedProductDefault</name>
        <dataType>String</dataType>
        <expression>if(Contains({!recordId.Profit_Center_New__c}, &quot;Google;&quot;) || Contains({!recordId.Profit_Center_New__c}, &quot;Bing;&quot;), &quot;Search New;VLA New;&quot;, &quot;&quot;)&amp;
