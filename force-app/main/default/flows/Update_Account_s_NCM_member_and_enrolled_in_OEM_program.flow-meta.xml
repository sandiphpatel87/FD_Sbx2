    <decisions>
        <name>Check_the_OEM_Program_Coop_or_NCM_Member_on_account</name>
        <label>Check the OEM Program Coop or NCM Member  on account</label>
        <locationX>314</locationX>
        <locationY>323</locationY>
        <defaultConnectorLabel>No</defaultConnectorLabel>
        <rules>
            <name>Check_the_OEM_Program_Coop</name>
            <conditionLogic>(1 OR 2) AND 3</conditionLogic>
            <conditions>
                <leftValueReference>$Record.OEM__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Acura</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.OEM__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Hyundai</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Type__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Digital Advertising</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_OEM_program_coop_checked_on_account</targetReference>
            </connector>
            <label>Check the OEM Program Coop</label>
        </rules>
        <rules>
            <name>Check_the_NCM_Member</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Type__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>NCM</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_NCM_Member_checked_on_account</targetReference>
            </connector>
            <label>Check the NCM Member</label>
        </rules>
    </decisions>
