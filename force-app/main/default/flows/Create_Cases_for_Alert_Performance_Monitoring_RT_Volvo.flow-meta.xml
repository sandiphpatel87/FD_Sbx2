    </formulas>
    <formulas>
        <name>IsFirstOfMonth</name>
        <dataType>Boolean</dataType>
        <expression>DAY(TODAY()) = 1</expression>
    </formulas>
    <formulas>
        <description>Is it Monday today ?</description>
        <name>ISMONDAY</name>
        <dataType>Boolean</dataType>
        <expression>WEEKDAY(TODAY()) = 2</expression>
    </formulas>
    <formulas>
        <name>TUESDAY</name>
        <dataType>Date</dataType>
        <expression>TODAY() + 1</expression>
    </formulas>
