    <decisions>
        <name>Check_the_Day</name>
        <label>Check the Day</label>
        <locationX>314</locationX>
        <locationY>252</locationY>
        <defaultConnector>
            <targetReference>Create_Cases</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Is_Monday</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>ISMONDAY</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Case_1_Assignment</targetReference>
            </connector>
            <label>Is Monday</label>
        </rules>
        <rules>
            <name>Is_Not_Monday</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>ISMONDAY</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Case_4_Assignment_0</targetReference>
            </connector>
            <label>Is Not Monday</label>
        </rules>
    </decisions>
