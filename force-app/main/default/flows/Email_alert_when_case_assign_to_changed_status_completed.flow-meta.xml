            </connector>
            <label>Case is closed</label>
        </rules>
        <rules>
            <name>When_Case_Status_is_Ready_For_QA</name>
            <conditionLogic>(1 OR 2 OR 3) AND 4 AND 5</conditionLogic>
            <conditions>
