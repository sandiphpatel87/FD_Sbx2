    <decisions>
        <name>Check_for_account_type_Website_verified_does_not_equal_to_Verified</name>
        <label>Check for account type &amp; Website verified not equal to &quot;Verified&quot;</label>
        <locationX>440</locationX>
        <locationY>492</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>If_Dig_Ad</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Opportunity__r.Type__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Digital Advertising</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Website_Verified__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Verified</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Measurement_Type__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Standard</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Create_a_case_for_Digital_Ad</targetReference>
            </connector>
            <label>If Dig Ad</label>
        </rules>
        <rules>
            <name>IfBI</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Opportunity__r.Type__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Business Intelligence</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Website_Verified__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Verified</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Measurement_Type__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Standard</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Create_a_case_for_BI</targetReference>
            </connector>
            <label>If BI</label>
        </rules>
    </decisions>
    <environments>Default</environments>
    <formulas>
        <name>Descriptionformula</name>
        <dataType>String</dataType>
        <expression>&quot;A new URL was added to this account. Verify that the URL is not dead. Friendly Reminder: A good URL Starts with https://www. Example: https://www.DEALERNAME.com (can end in .com, .net etc)&quot;</expression>
    </formulas>
    <formulas>
        <description>URL not empty</description>
        <name>URLisnotempty</name>
        <dataType>String</dataType>
        <expression>&quot;ISBLANK($Record.Account_Website_URL__c)&quot;</expression>
    </formulas>
