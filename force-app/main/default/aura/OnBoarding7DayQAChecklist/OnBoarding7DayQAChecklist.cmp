        <br/>
        
        <h3 class="slds-section-title--divider">Google QAs</h3>
        <span><lightning:input aura:id="checkboxField" label="Check Search Impression Share and CPCs" checked="{!v.onboardRecord.Check_Search_Impression_Share_and_CPCs__c}" type="checkbox" onchange='{!c.change}'/></span>
