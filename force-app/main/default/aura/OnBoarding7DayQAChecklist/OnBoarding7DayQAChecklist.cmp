        <br/>
        
        <h3 class="slds-section-title--divider">PMAX QAs</h3>
        <span><lightning:input aura:id="checkboxField" label="Make sure campaign is serving as intended" checked="{!v.onboardRecord.Make_sure_campaign_is_serving_as_intende__c}" type="checkbox" onchange='{!c.change}'/></span>
